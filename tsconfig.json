{
	"extends": "astro/tsconfigs/strict",
	"compilerOptions": {
		"target": "ES6",
		"module": "ESNext",
		"strict": true,
		"allowJs": true,
		"sourceMap": true,
		"skipLibCheck": true,
		"esModuleInterop": true,
		"resolveJsonModule": true,
		"moduleResolution": "bundler",
		"forceConsistentCasingInFileNames": true
	},
	"include": ["src"]
}
